    "paths": {
        "/etudiants/enregistrer": {
            "post": {
                "tags": [
                    "Étudiants"
                ],
                "summary": "Enregistrer un étudiant",
                "operationId": "f1cfdc767db9b8c05906e24896f8cbb5",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "etudiant",
                                    "parents"
                                ],
                                "properties": {
                                    "etudiant": {
                                        "properties": {
                                            "nom": {
                                                "type": "string",
                                                "example": "Doe"
                                            },
                                            "prenom": {
                                                "type": "string",
                                                "example": "John"
                                            },
                                            "adresse": {
                                                "type": "string",
                                                "format": "email",
                                                "example": "johndoe@example.com"
                                            },
                                            "num_tel": {
                                                "type": "string",
                                                "example": "1234567890"
                                            },
                                            "quartier": {
                                                "type": "string",
                                                "example": "Downtown"
                                            },
                                            "date_naissance": {
                                                "type": "string",
                                                "format": "date",
                                                "example": "2000-01-01"
                                            },
                                            "série": {
                                                "type": "string",
                                                "example": "A"
                                            },
                                            "niveau_academique": {
                                                "type": "string",
                                                "example": "Licence"
                                            }
                                        },
                                        "type": "object"
                                    },
                                    "parents": {
                                        "properties": {
                                            "full_name": {
                                                "type": "string",
                                                "example": "Jane Doe"
                                            },
                                            "occupation": {
                                                "type": "string",
                                                "example": "Doctor"
                                            },
                                            "num_tel": {
                                                "type": "string",
                                                "example": "0987654321"
                                            },
                                            "addresse": {
                                                "type": "string",
                                                "format": "email",
                                                "example": "janedoe@example.com"
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Étudiant enregistré avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Étudiant enregistré avec succès"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Validation des données échouée",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Les données envoyées ne sont pas valides."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur serveur",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Erreur lors de la création de l'étudiant"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
